<% for group in &difficulty.item_priorities { %>
    <% if group.name != "Default" && group.items.len() > 0 { %>
        <div class="row">
            <div class="col-1"><%= group.name %>:</div>
            <div class="col-11">
                <%= group.items.join(", ") %>
            </div>
        </div>
    <% } %>
<% } %>
